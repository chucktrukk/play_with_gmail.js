{
   "manifest_version": 2,
   "name": "Gmail Hacks",
   "description": "Experimental extension to play with Gmail.js by Kartik Talwar.",
   "permissions": ["activeTab"],
   "version": "0.1",
   "homepage_url": "http://browsernative.com/extensions/",
   "content_scripts": [ {
      "js": [ "script.js"],
      "css": ["style.css"],
      "matches": ["*://mail.google.com/*"],
      "run_at": "document_end"
   }
   ],
  "icons": {
      "128": "icon128.png"
    }
}